{
  "name": "mocha-grommet-reporter",
  "version": "1.5.10",
  "description": "Mocha Web UI built in React/Grommet",
  "main": "./dist/reporter.js",
  "scripts": {
    "prepublish": "cross-env NODE_ENV=prod npm run dist",
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch dist:watch",
    "open:src": "babel-node tools/srcServer.js",
    "dist": "babel src -d dist",
    "dist:watch": "nodemon --ignore dist --exec 'npm run dist'",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha --opts ./.mocharc --require babel-register \"dist/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "files": [
    "dist"
  ],
  "author": "Michael Plazek",
  "dependencies": {
    "cross-env": "^5.0.1",
    "grommet": "^1.4.1",
    "grommet-addons": "*",
    "jsdom": "^11.0.0",
    "lodash.chunk": "^4.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.0",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.8.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "^5.0.1",
    "eslint-watch": "^2.1.11",
    "eventsource-polyfill": "^0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "mocha": "^3.4.2",
    "nock": "8.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "1.8.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelplazek/mocha-grommet-reporter.git"
  }
}
